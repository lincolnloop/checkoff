{
    "es5": true,
    "esnext": true,
    "bitwise": false,
    "curly": false,
    "eqeqeq": true,
    "eqnull": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "undef": true,
    "strict": true,
    "trailing": true,
    "smarttabs": true,

    // Predefined globals whom JSHint will ignore.
    "browser": true,
    "node": true,
    "jquery": true,
    "predef": [
        "define",
        "requirejs"
    ]
}
